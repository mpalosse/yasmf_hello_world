{
  "name": "yasmf/yasmf_hello_world",
  "config": {
    "vendor-dir": "hello_world/lib/vendor"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/mpalosse/yasmf"
    }
  ],
  "autoload": {
    "psr-4": {
      "controllers\\": "hello_world/controllers",
      "application\\": "hello_world/application"
    }
  },
  "require": {
    "php": ">=8.4",
    "phpunit/phpunit": "13.0.*",
    "phpstan/phpstan": "1.10.*",
    "ext-pdo": "*",
    "yasmf/yasmf": ">=1.6.3"
  }
}
